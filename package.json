{
  "name": "vscode-agile-board",
  "version": "0.2.0",
  "engines": {
    "vscode": "^1.23.0"
  },
  "publisher": "sqrdup",
  "repository": {
    "type": "git",
    "url": "https://github.com/sqrdcodemons/vscode-agile-board"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "vscode-agile",
    "vsagile",
    "todo",
    "agile",
    "board",
    "note",
    "notes"
  ],
  "activationEvents": [
    "onCommand:vsagile.startDummy",
    "onCommand:vsagile.start",
    "onCommand:vsagile.setupBoard"
  ],
  "main": "./build/ext-src/extension.js",
  "license": "MIT",
  "contributes": {
    "configuration": {
      "type": "object",
      "title": "VSAgile Board Configuration",
      "properties": {
        "vsagile.defaultBoardPath": {
          "type": "string",
          "default": "",
          "description": "Path to directory to save boards. Use ~/ to denote a relative path from home folder."
        }
      }
    },
    "commands": [
      {
        "command": "vsagile.startDummy",
        "title": "VSAgile: Start Dummy Board"
      },
      {
        "command": "vsagile.start",
        "title": "VSAgile: Start New Board"
      },
      {
        "command": "vsagile.setupBoard",
        "title": "VSAgile: Run setup"
      }
    ]
  },
  "extensionDependencies": [
    "yzhang.markdown-all-in-one"
  ],
  "dependencies": {
    "fs-extra": "^9.0.1",
    "moment": "^2.29.1",
    "node-sass": "^4.0.0",
    "react": "^16.14.0",
    "react-beautiful-dnd": "^13.0.0",
    "react-dom": "^16.3.2",
    "react-icons": "^4.1.0",
    "react-transition-group": "^4.4.1",
    "react-window": "^1.8.6",
    "terser": "3.16.0",
    "vscode": "^1.1.37"
  },
  "resolutions": {
    "terser": "3.14.1"
  },
  "scripts": {
    "vscode:prepublish": "./scripts/build-non-split.js && tsc -p tsconfig.extension.json",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "start": "react-scripts start",
    "build": "./scripts/build-non-split.js && tsc -p tsconfig.extension.json",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "devDependencies": {
    "@emotion/react": "^11.1.1",
    "@emotion/styled": "^11.0.0",
    "@types/fs-extra": "^8.1.0",
    "@types/jest": "^23.3.13",
    "@types/moment": "^2.13.0",
    "@types/node": "^10.1.2",
    "@types/react": "^16.14.2",
    "@types/react-beautiful-dnd": "^13.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-transition-group": "^4.4.0",
    "@types/react-window": "^1.8.2",
    "react-scripts": "^2.1.3",
    "rewire": "^5.0.0",
    "typescript": "^3.3.1"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
